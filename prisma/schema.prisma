datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Category {
  id    Int     @id @default(autoincrement())
  name  String
  books Book[]
}

model Book {
  id         Int      @id @default(autoincrement())
  title      String
  categoryId Int
  category   Category @relation(fields: [categoryId], references: [id])
}